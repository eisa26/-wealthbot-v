<form id="retirement_account_fund_form" action="{{ path("rx_ria_client_create_outside_fund") }}" method="post" >
    <table>
        <tr>
            <td>{{ form_label(form.name) }}</td>
            <td>{{ form_label(form.symbol) }}</td>
        </tr>
        <tr>
            <td>
                {{ form_errors(form.name) }}
                {{ form_widget(form.name) }}
            </td>
            <td>
                {{ form_errors(form.symbol) }}
                {{ form_widget(form.symbol) }}
            </td>
        </tr>
        <tr>
            <td colspan="2">{{ form_rest(form) }}</td>
        </tr>
        <tr>
            <td colspan="2"><input class="btn btn-info" type="submit" name="submit" value="Add"/></td>
        </tr>
    </table>
</form>