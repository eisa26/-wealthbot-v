<form id="ria_documents_form" action="{{ path('rx_ria_change_profile_upload_documents') }}" method="post" enctype="multipart/form-data">
    {{ form_errors(form) }}

    {% for key, field in form %}
        {% if key != '_token' %}
            <div class="margin-top-10 clearfix">
                <h3>{{ field.vars.label == 'ADV Part 2' ? 'ADV' : field.vars.label }}</h3>
                {{ form_errors(field) }}
                {{ form_widget(field) }}

                {% if documents[key] is defined %}
                    <a href="{{ path('rx_download_document', {'filename': documents[key].filename}) }}" target="_blank">{{ documents[key].name == 'Adv' ? 'Advisor ADV' : documents[key].name }} <i class="icon-download"></i></a>
                {% endif %}
            </div>
        {% endif %}
    {% endfor %}

    {{ form_rest(form) }}
    <div class="form-group">
        <input class="btn btn-primary btn-primary"  type="submit" value="Save"/>
    </div>
</form>