{% extends '/Admin/Pas/layout.html.twig' %}

{% block tab_content %}
    <table class="table pas-sec-files">
        <thead>
            <th>#</th>
            <th>RIA</th>
            <th>Custodian</th>
            <th>Last Name</th>
            <th>First Name</th>
            <th>Acct Number</th>
            <th>Symbol Name</th>
            <th>Webo Shares</th>
            <th>Webo Cost Basis</th>
            <th>Webo Date</th>
            <th>Custodial Shares</th>
            <th>Custodial Cost Basis</th>
            <th>Custodial Date</th>
        </thead>
        <tbody>
            {% for row in data %}
                <tr>
                    <td>{{ loop.index }}</td>
                    <td>{{ row.ria }}</td>
                    <td>{{ row.custodian }}</td>
                    <td>{{ row.last_name }}</td>
                    <td>{{ row.first_name }}</td>
                    <td>{{ row.acct_number }}</td>
                    <td>{{ row.symbol }}</td>
                    <td>{{ row.ce_shares }}</td>
                    <td>{{ row.ce_cost_basis }}</td>
                    <td>{{ row.ce_date }}</td>
                    <td>{{ row.custodial_shares }}</td>
                    <td>{{ row.custodial_cost_basis }}</td>
                    <td>{{ row.custodial_date }}</td>
                </tr>
            {% endfor %}
        </tbody>
    </table>
    <div class="pagination pagination-centered">
        {{ knp_pagination_render(pagination) }}
    </div>
{% endblock %}