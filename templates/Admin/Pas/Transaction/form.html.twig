<div class="modal top modal-800w ce-form-modal" id="transactionModal">
    <div id="inside">
    <div calss="modal-content">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h3>Add transaction</h3>
    </div>

    <div class="modal-body">
        <form method="post" {{ form_start(form) }}>
            {{ form_row(form._token) }}
            <div class="row">
                <div class="col-4">
                    <div class="form-group">
                        {{ form_label(form.advisorCode, 'Advisor code', {'label_attr': {'class': 'control-label'}}) }}
                        <div class="controls">
                            {{ form_widget(form.advisorCode, {'attr': {'class': '', 'data-role': 'autocomplete', 'data-route': 'rx_admin_pas_interfaces_advisor_code_autocomplete' }}) }}
                            {{ form_errors(form.advisorCode) }}
                        </div>
                    </div>
                    <div class="form-group">
                        {{ form_label(form.securityCode, 'Security code', {'label_attr': {'class': 'control-label'}}) }}
                        <div class="controls">
                            {{ form_widget(form.securityCode, {'attr': {'class': '', 'data-role': 'autocomplete', 'data-route': 'rx_admin_pas_interfaces_security_type_autocomplete' }}) }}
                            {{ form_errors(form.securityCode) }}
                        </div>
                    </div>
                    <div class="form-group">
                        {{ form_label(form.grossAmount, 'Gross amount', {'label_attr': {'class': 'control-label'}}) }}
                        <div class="controls">
                            {{ form_widget(form.grossAmount, {'attr': {'class': ''}}) }}
                            {{ form_errors(form.grossAmount) }}
                        </div>
                    </div>
                    <div class="form-group">
                        {{ form_label(form.txDate, 'Tx date', {'label_attr': {'class': 'control-label'}}) }}
                        <div class="controls">
                            {{ form_widget(form.txDate, {'attr': {'class': 'jq-us-date', 'placeholder': 'm/d/Y'}}) }}
                            {{ form_errors(form.txDate) }}
                        </div>
                    </div>
                </div>
                <div class="col-4">
                    <div class="form-group">
                        {{ form_label(form.accountNumber, 'Account number', {'label_attr': {'class': 'control-label'}}) }}
                        <div class="controls">
                            {{ form_widget(form.accountNumber, {'attr': {'class': '', 'data-role': 'autocomplete', 'data-route': 'rx_admin_pas_interfaces_account_number_autocomplete' }}) }}
                            {{ form_errors(form.accountNumber) }}
                        </div>
                    </div>
                    <div class="form-group">
                        {{ form_label(form.closingMethod, 'Closing method', {'label_attr': {'class': 'control-label'}}) }}
                        <div class="controls">
                            {{ form_widget(form.closingMethod, {'attr': {'class': '', 'data-role': 'autocomplete', 'data-route': 'rx_admin_pas_interfaces_closing_method_autocomplete'}}) }}
                            {{ form_errors(form.closingMethod) }}
                        </div>
                    </div>
                    <div class="form-group">
                        {{ form_label(form.netAmount, 'Net amount', {'label_attr': {'class': 'control-label'}}) }}
                        <div class="controls">
                            {{ form_widget(form.netAmount, {'attr': {'class': ''}}) }}
                            {{ form_errors(form.netAmount) }}
                        </div>
                    </div>
                    <div class="form-group">
                        {{ form_label(form.settleDate, 'Settle date', {'label_attr': {'class': 'control-label'}}) }}
                        <div class="controls">
                            {{ form_widget(form.settleDate, {'attr': {'class': 'jq-us-date', 'placeholder': 'm/d/Y'}}) }}
                            {{ form_errors(form.settleDate) }}
                        </div>
                    </div>
                </div>
                <div class="col-4">
                    <div class="form-group">
                        {{ form_label(form.transactionCode, 'Transaction code', {'label_attr': {'class': 'control-label'}}) }}
                        <div class="controls">
                            {{ form_widget(form.transactionCode, {'attr': {'class': '', 'data-role': 'autocomplete', 'data-route': 'rx_admin_pas_interfaces_transaction_type_autocomplete' }}) }}
                            {{ form_errors(form.transactionCode) }}
                        </div>
                    </div>
                    <div class="form-group">
                        {{ form_label(form.symbol, 'Symbol', {'label_attr': {'class': 'control-label'}}) }}
                        <div class="controls">
                            {{ form_widget(form.symbol, {'attr': {'class': '', 'data-role': 'autocomplete', 'data-route': 'rx_admin_pas_interfaces_security_autocomplete' }}) }}
                            {{ form_errors(form.symbol) }}
                        </div>
                    </div>
                    <div class="form-group">
                        {{ form_label(form.qty, 'Quantity', {'label_attr': {'class': 'control-label'}}) }}
                        <div class="controls">
                            {{ form_widget(form.qty, {'attr': {'class': ''}}) }}
                            {{ form_errors(form.qty) }}
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-12">
                    <div class="form-group">
                        {{ form_label(form.notes, 'Notes', {'label_attr': {'class': 'control-label'}}) }}
                        <div class="controls">
                            {{ form_widget(form.notes, {'attr': {'class': 'col-12'}}) }}
                            {{ form_errors(form.notes) }}
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-actions">
                <div class="controls">
                    <button type="submit" class="btn btn-primary">Submit</button>
                    <button type="button" class="btn btn-primary"  data-dismiss="modal">Close</button>
                </div>
            </div>
        </form>
        </div>
      </div>
    </div>
</div>