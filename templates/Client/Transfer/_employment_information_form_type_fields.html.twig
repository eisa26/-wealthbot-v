                <div class="col-2">
                    {{ form_errors(form.spouse_last_name) }}
                    {{ form_widget(form.spouse_last_name, { 'attr': { 'class': 'input-medium', 'placeholder': 'Last Name' } }) }}
                </div>
                <div class="col-12">
                    {{ form_label(form.spouse_birth_date, 'Spouse Date of Birth') }}
                    {{ form_errors(form.spouse_birth_date) }}
                    {{ form_widget(form.spouse_birth_date, {'attr': { 'placeholder': 'MM-DD-YYYY' }}) }}
                </div>
            </div>
        </div>
    </div>
{% endif %}
