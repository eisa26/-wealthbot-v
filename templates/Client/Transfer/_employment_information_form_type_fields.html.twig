<div class="form-group row">
    <div class="span">
        {{ form_label(form.employment_type, 'Employment Status') }}
        <div class="form-inline employment-status">
            {{ form_errors(form.employment_type) }}
            {% for radio in form.employment_type %}
                <label for="{{ radio.vars.id }}"accesskey=""  class="checkbox inline">
                    {{ form_widget(radio) }}
                </label>
            {% endfor %}
        </div>
    </div>
</div>
